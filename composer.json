{
    "name": "gustavo82mdq/eav",
    "type": "library",
    "description": "Eav",
    "keywords": [
        "Gustavo82mdq",
        "Eav"
    ],
    "homepage": "https://github.com/Gustavo82mdq/Eav",
    "license": "MIT",
    "authors": [
        {
            "name": "gustavo82mdq",
            "email": "gustavo82mdq@gmail.com",
            "homepage": "https://github.com/gustavo82mdq",
            "role": "Developer"
        }
    ],
        "minimum-stability" : "dev",
        "prefer-stable" : true,
    "require": {
        "php": "~5.6|~7.0",
        "backpack/crud": "^3.2",
        "backpack/generators": "^1.1",
        "illuminate/support": "~5.1",
        "laravel/framework": "5.*",
        "rinvex/attributes": "^0.0.3"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Gustavo82mdq\\Eav\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gustavo82mdq\\Eav\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
